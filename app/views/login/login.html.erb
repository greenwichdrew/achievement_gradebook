<h1>Login page</h1>
<%= form_tag login_login_path do %>
  <%= text_field_tag :email, nil, placeholder: "email" %>
  <%= password_field_tag :password, nil, placeholder: "password" %>
  <%= select_tag :type, options_for_select(["Teacher", "Parent", "Student"]), include_blank: true %>
  <%= submit_tag  "login" %>
<% end %>
